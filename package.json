{
  "name": "graphpql-poc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "serve": "concurrently -k \"nodemon ./packages/services/products-graph/index.js\" \"nodemon ./packages/services/reviews-graph/index.js\" \"nodemon ./packages/s2s-graph/index.js\"",
    "start:products": "nodemon ./packages/services/products-graph/index.js",
    "start:reviews": "nodemon ./packages/services/reviews-graph/index.js",
    "start:inventory": "nodemon ./packages/services/inventory-service/index.js",
    "start:acl": "nodemon ./packages/acl-graph/index.js",
    "start:s2s": "nodemon ./packages/s2s-graph/index.js",
    "babel-node": "babel-node --presets='@babel/preset-env' --ignore='foo|bar|baz'"
  },
  "dependencies": {
    "@apollo/gateway": "^2.5.6",
    "@apollo/server": "^4.9.4",
    "@apollo/subgraph": "^2.5.6",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "concurrently": "^3",
    "nodemon": "^3.0.1"
  }
}
